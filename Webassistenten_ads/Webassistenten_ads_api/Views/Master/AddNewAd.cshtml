    {
        string correctFormat = @a.DatoUtgivelse.Year+"-"
            +@a.DatoUtgivelse.Month+"-"
            +@a.DatoUtgivelse.Day;
    <option name="@correctFormat">@correctFormat</option>
    }
        </select>
    
    
    
    <hr />
    /*Non-mandatory fields below:*/
