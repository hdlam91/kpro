    <label for="Type">Type</label>
    
    <input class="text-box single-line" data-val="true" data-val-number="The field Type must be a number." id="Type" name="Type" type="number" value="" /><br>
    <label for="Price">Price</label>
    
    <input class="text-box single-line" data-val="true" data-val-number="The field Price must be a number." id="Price" name="Price" type="number" value="" /><br>
