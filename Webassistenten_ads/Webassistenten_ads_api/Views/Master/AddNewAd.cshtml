@model Webassistenten_ads_api.Models.HD

@{
    ViewBag.Title = "Add a new ad";
}

@{
    IEnumerable <Webassistenten_ads_api.ProduktUtgivelse> pd = Model.GetDate();
 

}




<h2>Test</h2>
@using (Html.BeginForm("Upload", "Master", FormMethod.Post, new { enctype = "multipart/form-data" }))
